#include <iostream>
//#include "Fibonacci.hpp"
#include "Vecteur3.hpp"

int main () {
  vecteur3 v{2,3,6};

  vecteur3 u{1,3,4};
  
  afficher(v);
  u.afficher();
  std::cout<<"La norme du vecteur v est : "<<norme(v)<<std::endl;
  addition(u,v).afficher();
  std::cout<<"Fibonacci récursifIteratif" << std::endl;
  std::cout<<"Le 7 ème terme de la suite de Fibonacci est " << fibonacciIteratif(7) << std::endl;
  std::cout<<"Fibonacci récursif"<< std::endl;
  std::cout<<"Le 7 ème terme de la suite de Fibonacci est " << fibonacciRecursif(7) << std::endl;
}
