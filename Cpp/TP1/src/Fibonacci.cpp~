#include <iostream>

int fibonacciRecursif (int n){
  if (n<=1){
    return n 
      }else {
    return fibonacciRecursif(n-1)+fibonacciRecursif(n-2)
  }
}

int fibonacciIteratif (n){
  int fibo0 = 0;
  int fibo1 = 1;
  int i = 2;
  int res;
  for(i; i < n ; i++){
    res += fibo0+fibo1
      fibo0 = i;
    fibo1=res;
  }
}

int main () {
  std::cout<<"Fibonacci récursifIteratif"<<endl;
   std::cout<<"Le 7 ème terme de la suite de Fibonacci est "<<fibonacciIteratif(7)<<endl;

  std::cout<<"Fibonacci récursif"<<endl;
  std::cout<<"Le 7 ème terme de la suite de Fibonacci est "<<fibonacciRecursi(7)<<endl;
}
